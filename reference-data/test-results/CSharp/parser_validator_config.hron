# Controls the parser validation step for CSharp parser
@Equivalence
	=EmptyContentLinesAndEmptyLines
		true
	=CommentsAndCommentLines
		true
=PreProcessor
	ValidateAll
=Object_Begin
	ValidateAll
=Object_End
	ValidateExistence
=Value_Begin
	ValidateAll
=Value_End
	ValidateExistence
=Comment
	ValidateContent
=Empty
	Ignore
=ContentLine
	ValidateAll
=EmptyLine
	ValidateExistence
=CommentLine
	ValidateContent
